cat farcast2-app.yaml | docker-compose exec -T kuma-cp2 kumactl apply -f -
docker-compose exec -T kuma-cp2 kumactl generate dataplane-token --dataplane=app | docker-compose exec -T kuma-cp2 tee /config/farcast2-app-token > /dev/null

docker-compose stop kuma-app2-dp kuma-app2
docker-compose rm -f kuma-app2-dp kuma-app2
docker-compose up -d kuma-app2-dp
docker-compose up -d kuma-app2
