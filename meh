cat traffic-permission-allow-all-traffic.yaml | docker-compose exec -T kuma-global kumactl apply -f -
cat mesh-default.yaml | docker-compose exec -T kuma-global kumactl apply -f -
cat fargate-zone.yaml | docker-compose exec -T kuma-global kumactl apply -f -
cat fargate-ingress.yaml | docker-compose exec -T kuma-cp kumactl apply -f -
docker-compose exec kuma-cp kumactl generate dataplane-token --dataplane=kuma-ingress | docker-compose exec -T kuma-cp tee /config/fargate-ingress-token
docker-compose stop kuma-ingress
docker-compose rm -f kuma-ingress
docker-compose up -d
